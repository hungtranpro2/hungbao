<div class="card">
  <div class="card-header border-0">
    <h3 class="card-title"><b><%= t ".title"%></b></h3>
    <% if @project.user_projects.where(role: 1).present? %>
     <h6 class="card-title" style="margin-left: 10px; color: #dc3545; font-weight: 900">- <%= t ".had"%>
     </h6>
    <% end %>
  </div>
  <div class="card-body table-responsive p-0">
    <table class="table table-striped table-valign-middle">
      <thead>
      <tr>
        <th>#</th>
        <th><%= t ".member"%></th>
        <th><%= t ".skill"%></th>
        <th><%= t ".join_project"%></th>
        <th class="text-center"><%= t ".more"%></th>
      </tr>
      </thead>
      <tbody>
        <%= render partial: "add_project_managers/user", collection: @users %>
      </tbody>
    </table>
  </div>
</div>
