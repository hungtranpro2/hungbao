<% provide(:title, t(".begin_title")) %>
<div class="content-wrapper" style="height: 500px;">
  <%= render partial: "shared/head_profile", locals: {path1: overview_companys_path, title: "Danh sách thành viên dự án", link1: "Quản lý dự án", link2: "Thành viên"}%>
  <section class="content">
    <div class="col-12">
    <div class="float-right" style="margin: 10px 1px;">
      <%= search_form_for @q, url:"#", class: "form-inline" do |f| %>
        <%= f.search_field :name_cont, class: "form-control mr-sm-2", placeholder: "Search" %>
        <%= f.submit class: "btn btn-outline-success my-2 my-sm-0" do %>
          <i class="fas fa-search"></i>
        <% end %>
      <% end %>
    </div>
     <div class="card" style="width: 100%">
        <div class="card-header border-0">
          <h3 class="card-title">Thêm quản lý dự án</h3>
        </div>
        <div class="card-body table-responsive p-0">
          <table class="table table-striped table-valign-middle">
            <thead>
            <tr>
              <th>#</th>
              <th>Member</th>
              <th>Division</th>
              <th>Role</th>
              <th>Join the project</th>
              <th>Progress</th>
              <th>Status</th>
              <th class="text-center">More</th>
            </tr>
            </thead>
            <tbody>
              <%= render partial: "company_members/user", collection: @users %>
            </tbody>
          </table>
        </div>
      </div>
      <%= will_paginate @users if @users %>
    </div>
  </section>
  <!-- /.content -->
</div>
