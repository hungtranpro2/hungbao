<div class="card">
  <div class="card-header border-0" style="height: 40px;">
    <h3 class="card-title"><b><%= t ".title"%></b></h3>
  </div>
  <div class="card-body table-responsive p-0">
    <table class="table table-striped table-valign-middle">
      <thead>
      <tr>
        <th>#</th>
        <th><%= t ".member"%></th>
        <th><%= t ".division"%></th>
        <th><%= t ".skill"%></th>
        <th><%= t ".join_project"%></th>
        <th class="text-center"><%= t ".more"%></th>
      </tr>
      </thead>
      <tbody>
        <%= render partial: "add_project_divisions/user", collection: @users %>
      </tbody>
    </table>
  </div>
</div>
